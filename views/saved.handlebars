<div class="row">
  <div class="col-md-12">
    <main>
      <div class="articles">
        {{#each saved}}
          <a href="{{this.link}}" target="_blank"><h3><strong>{{this.title}}</strong></h3></a>
          <!-- <p>{{this.summary}}</p> -->
          <a href="{{this.link}}" target="_blank"><p>{{this.link}}</p></a>
          <button value="{{this._id}}" class="btn btn-primary unsave-btn">Unsave Article</button>
          <button value="{{this._id}}" class="btn btn-secondary note-btn">Add A Note</button>
        {{/each}}
      </div>
    </main>
  </div>
</div>
