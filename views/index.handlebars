<div class="row">
  <div class="col-md-12">
    <main>
      <div class="articles">
        {{#each articles}}
            <a href="{{this.link}}" target="_blank"><h3><strong>{{this.title}}</strong></h3></a>
            <!-- <p>{{this.summary}}</p> -->
            <a href="{{this.link}}" target="_blank"><p>{{this.link}}</p></a>
            <button class="btn btn-primary save-btn">Save Article</button>
        {{/each}}
      </div>
    </main>
  </div>
</div>
